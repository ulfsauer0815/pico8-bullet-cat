pico-8 cartridge // http://www.pico-8.com
version 21
__lua__
-- Bullet Cat
-- by Ulf Sauer


local PS_LEFT=3
local PS_RIGHT=4
local PS_UP=1
local PS_DOWN=2
local JUMP_TIME=20

local player

function _init()
	player={
		x=64,
		y=50,
		sprite=1,
		jump_energy=0,
		vy=0
	}
end

function on_ground()
	return player.y >= 56
end

function _update()
	if(btn(2)) then
		player.sprite=PS_DOWN
		--player.y-=2
	end
	if(btn(3)) then
		 player.sprite=PS_UP
		--player.y+=2
	end
	if(btn(0)) then
		player.sprite=PS_LEFT
		player.x-=2
	end
	if(btn(1)) then
		player.sprite=PS_RIGHT
		player.x+=2
	end
	if(btnp(4) and on_ground()) then
		player.jump_energy=10
	end
	if(player.jump_energy > 0 or not on_ground()) then
		if (player.jump_energy>0) then
			player.vy=2
			player.y-=player.vy
		else
			player.vy=-1
			player.y-=player.vy
		end
		player.jump_energy-=1
	end
end

function _draw()
	cls()
	map(0,0,0,0,16,16)
	spr(player.sprite,player.x,player.y)
end

